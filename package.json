{
  "name": "123hansa-marketplace",
  "version": "1.0.0",
  "description": "123hansa.se - Nordic Business Marketplace Platform",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:web\"",
    "dev:web": "cd apps/web && npm run dev",
    "dev:api": "cd apps/api && npm run dev",
    "build": "npm run build:web && npm run build:api",
    "build:web": "cd apps/web && npm run build",
    "build:api": "cd apps/api && npm run build",
    "test": "npm run test:web && npm run test:api",
    "test:web": "cd apps/web && npm run test",
    "test:api": "cd apps/api && npm run test",
    "test:website": "node scripts/simple-test-runner.js",
    "test:sentry": "node scripts/manual-sentry-test.js",
    "test:staging": "node scripts/staging-test-runner.js",
    "test:staging:sentry": "node scripts/staging-sentry-test.js",
    "test:local": "node scripts/local-comprehensive-test.js",
    "test:api": "node scripts/test-api-server.js",
    "scan:full": "node scripts/website-scanner.js",
    "lint": "npm run lint:web && npm run lint:api",
    "lint:web": "cd apps/web && npm run lint",
    "lint:api": "cd apps/api && npm run lint"
  },
  "workspaces": [
    "apps/*"
  ],
  "keywords": [
    "marketplace",
    "business",
    "nordic",
    "sweden",
    "norway",
    "denmark",
    "flippa",
    "b2b"
  ],
  "author": "123hansa Team",
  "license": "UNLICENSED",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "node-fetch": "^3.3.2",
    "puppeteer": "^21.5.0"
  },
  "dependencies": {
    "@sentry/node": "^9.36.0",
    "@sentry/profiling-node": "^9.36.0",
    "@sentry/react": "^9.36.0",
    "@sentry/vite-plugin": "^3.5.0",
    "cors": "^2.8.5",
    "express": "^4.21.2"
  }
}